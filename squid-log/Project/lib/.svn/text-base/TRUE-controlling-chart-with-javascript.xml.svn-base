<settings>
  <type>bar</type>
  <text_size>9</text_size>
  <data_type>csv</data_type>
  <background>
    <!--<alpha>100</alpha>
    <border_alpha>20</border_alpha>-->
    <file>true.jpg</file>
  </background>
  <grid>
    <category>
      <dashed>1</dashed>
    </category>
    <value>
      <dashed>1</dashed>
    </value>
  </grid>
  <axes>
    <category>
      <width>1</width>
      <color>E7E7E7</color>
    </category>
