      <width>1</width>
      <color>E7E7E7</color>
    </value>
  </axes>
  <values>
    <value>
      <min>0</min>
    </value>
  </values>
  <column>
    <type>stacked</type>
    <width>85</width>
    <hover_brightness>40</hover_brightness>
    <hover_color>00FFFF</hover_color>
    <balloon_text>{title}: {value}%</balloon_text>
    <grow_time>3</grow_time>
  </column>
  <depth>15</depth>
  <graphs>
    <graph gid="0">
      <title>Success</title>
      <color>ADD981</color>
    </graph>
    <graph gid="1">
      <title>Failed</title>
      <color>ff6347</color>
    </graph>
    <graph gid="2">
